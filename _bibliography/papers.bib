@INPROCEEDINGS{Singh2021xg,
  title     = {{LEx: A Framework for Operationalising Layers of Machine Learning
               Explanations}},
  author    = {Singh, Ronal and Ehsan, Upol and Cheong, Marc and Riedl, Mark O
               and Miller, Tim},
  booktitle = {{ACM CHI 2023 Workshop on Human-Centered Explainable AI (HCXAI)}},
  year      = {2021},
  abstract  = {Several social factors impact how people respond to AI
               explanations used to justify AI decisions affecting them
               personally. In this position paper, we define a framework called
               the \textit{layers of explanation} (LEx), a lens through which we
               can assess the appropriateness of different types of
               explanations. The framework uses the notions of
               \textit{sensitivity} (emotional responsiveness) of features and
               the level of \textit{stakes} (decision's consequence) in a domain
               to determine whether different types of explanations are
               \textit{appropriate} in a given context. We demonstrate how to
               use the framework to assess the appropriateness of different
               types of explanations in different domains.},
